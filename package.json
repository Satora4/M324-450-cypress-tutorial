  "scripts": {
    "dev": "vite",
    "vite-build": "vite build",
    "test": "jest",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "increment-version": "node increment-version.js",
    "clean": "rimraf dist",
    "prepare-build": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist --extensions \".js,.jsx\"",
    "copy": "node -e \"const fs = require('fs'); fs.copyFileSync('package.json', 'dist/package.json'); fs.copyFileSync('README.md', 'dist/README.md');\"",
    "build": "npm run increment-version && npm run prepare-build && npm run copy"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@babel/cli": "^7.26.4",
